{
  "name": "starter-npm-package",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.10.2",
  "description": "Opinionated NPM package starter",
  "author": "EstÃ©ban Soubiran <e.soubiran25@gmail.com> (https://github.com/barbapapazes/)",
  "license": "MIT",
  "homepage": "https://github.com/barbapapazes/starter-npm-package",
  "keywords": [
    "starter",
    "npm",
    "package"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "release": "bumpp",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@antfu/eslint-config": "2.3.4",
    "@types/node": "^20.9.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "jiti": "^1.21.0",
    "unbuild": "^2.0.0"
  }
}
